{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "arrow-parens": 0,
    "quotes": 0,
    "no-console": 0,
    "no-unused-vars": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "import/no-extraneous-dependencies": 0,
    "quote-props": [0, "consistent-as-needed"],
    "object-shorthand": [0, "properties"]
  }
}
